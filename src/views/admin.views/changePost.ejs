<div class="container my-3" style="text-align: center;">
    <h2>Change Post</h2>
 </div>
 <hr>
 
 <div class="container">
 <table class=" table table-success">
        <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Post</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <% if(users){%>
            <tbody>
              <% for(user of users){%>
                <tr>
                  <td><%=user.userName %></td>
                  <td><%=user.userEmail %></td>
                  <td><%=user.userRole %></td>
                  <td><a href="/admin/changepost/<%= user._id %>" class="button primary">Make admin</a></td>
                </tr>
              <%}%>
             
            </tbody>
          <%}%>
    
          
    </table>
</div>
